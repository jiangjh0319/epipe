const Util = {
  getNewTime: function (date,mark) {
    var datesave = new Date(date);
    if (mark == '年') {
      return datesave.getFullYear() + '年' + (datesave.getMonth() + 1) + '月' + datesave.getDate() + '日'
    } else {
      return datesave.getFullYear() + '-' + (datesave.getMonth() + 1) + '-' + datesave.getDate()
    }
  }
}
module.exports = Util
