const Service = {
  host: 'http://spd9.com',
  news: '/news/indurstryinfo',
  queryString: function (params, postFlag) {
    var paramUrl = ''
    for (var key in params) {
      if (!postFlag && paramUrl == '') {
        paramUrl += '?' + key + '=' + params[key]
      } else {
        paramUrl += '&' + key + '=' + params[key]
      }
    }
    return paramUrl
  },
  name: '张SSS三'
}
module.exports = Service
